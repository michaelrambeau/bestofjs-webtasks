{
  "extends": "standard",
  "rules": {
    "indent": [2, 2],
    "comma-dangle": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "curly": 0,
    "default-case": 0,
    "func-names": 0,
    "no-use-before-define": 0,
    "camelcase": 0,
    "quotes": 1,
    "dot-notation": 0,
    "no-console": 0,
    "no-else-return": 0
  }
}
